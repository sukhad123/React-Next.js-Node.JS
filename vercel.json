{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", // Route API requests to your server.js
        "dest": "./server.js"
      },
      {
        "src": "/(.*)", // Redirect all other requests to localhost:3000
        "dest": "http://localhost:3000"
      }
    ]
  }
  